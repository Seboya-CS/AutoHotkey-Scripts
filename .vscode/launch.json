    "version": "0.2.0",
    "configurations": [
        {
            "name": "AHK Debug",
            "type": "autohotkey",
            "request": "launch",
            "runtime": "C:\\Program Files\\AutoHotkey\\v2\\AutoHotkey64.exe",
            "stopOnEntry": true,
            "useAutoJumpToError": true,
            "useAnnounce": "detail",
            "useIntelliSenseInDebugging": false,
            "useOutputDebug": true,
            "program": "${file}"
        }
    ]
}